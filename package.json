    "@babel/preset-env": "^7.3.4",
    "browser-sync": "^2.26.3",
    "cssnano": "^4.1.10",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-file-include": "^2.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-import-css": "^0.1.3",

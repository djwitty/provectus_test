  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "Gulp 001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://mikhail_pov@bitbucket.org/mikhail_pov/gulp001.git"
  },
  "config": {
    "src": {
      "sass": "src/sass/main.sass",
      "html": "src",
      "js": "src/js/main.js",
      "fonts": "src/fonts/**/*.*",
      "img": {
        "all:": [
          "src/img/**/*.*",
          "!src/img/noCompress/**/*.*"
        ],
        "noCompress": "src/img/noCompress/**/*.*"
      }
    },
    "dist": {
      "css": "dist/css",
      "html": "dist",
      "js": "dist/js",
      "fonts": "dist/fonts",
      "img": "dist/img"
    },
    "libs": {
      "js": "src/js/libs.js",
      "css": "src/css/libs.css"
    }
  },
